class Solution {
    public int[] searchRange(int[] nums, int target) {
        int left=0;
        int right=nums.length-1;
        int mid=0;
        int []res={-1,-1};

        while(left<=right){
            mid=(left+right)/2;
            if(target==nums[mid]){
                res[0]=mid;
                right=mid-1;
            }
            else if(nums[mid]<target){
                left=mid+1;
            }
            else{
                right=mid-1;   
            } 
        }
        left=0;
        right=nums.length-1;
        mid=0;
        while(left<=right){
            mid=(left+right)/2;
            if(target==nums[mid]){
                res[1]=mid;
                left=mid+1;
            }
            else if(nums[mid]<target){
                left=mid+1;
            }
            else{
                right=mid-1;
            }
        }
        return res;


        
    }
}
