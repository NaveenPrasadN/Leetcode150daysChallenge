class Solution {
    public int firstUniqChar(String s) {
        HashMap<Character,Integer> n =new HashMap<>();

        for(char a:s.toCharArray()){
            n.put(a,n.getOrDefault(a,0)+1);
        }
        for(int i=0;i<s.length();i++){
            if(n.get(s.charAt(i))==1){
                return i;
            }
        }
        return -1;
        
    }
}
